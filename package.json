{
  "name": "redux-reuse-recycle",
  "version": "1.0.0",
  "description": "A DSL to generate Redux boilerplate code.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "ts-node src",
    "build": "npx rollup -c",
    "tsc-build": "tsc src/**/*.ts",
    "prepublish": "npm run build",
    "test": "mocha -r ts-node/register ./test/*",
    "test-file": "mocha -r ts-node/register ./test/File.spec.ts --",
    "test-parse": "mocha -r ts-node/register ./test/Parser.spec.ts",
    "test-typecheck": "mocha -r ts-node/register ./test/TypeCheck.spec.ts"
  },
  "keywords": [
    "redux",
    "dsl",
    "boilerplate"
  ],
  "author": "Gina Bolognesi, Jeffrey Doyle, Gregory Gzik, Brian Neumann, Jessica Yang",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@types/chai": "^4.2.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.8",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "rollup": "^1.16.7",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-node-register": "^1.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {}
}
